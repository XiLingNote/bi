	width: 100%;
	padding: 1px;
/* 	min-width: 843px;
 */}

.btnT {
	display: inline-block;
	padding: 4px 4px;
	margin-bottom: 0;
	font-size: 12px;
	font-weight: normal;
	line-height: 1.428571429;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	cursor: pointer;
	border: 1px solid transparent;
	border-radius: 4px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
}

#gmvTarget {
	background: pause;
	width: 100%;
	height: 73.5%;
}

#targetCountAndValue {
	width: 30%;
	position: absolute;
	margin-left: 2%;
	margin-top: 2%;
}

