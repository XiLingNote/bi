@CHARSET "UTF-8";

/* 字体 */
#cusTable {
	background-color: #A9A9A9;
	color: black;
}

.bootstrap-table {
	color: black;
}
/* 表格表头可以自定义长度 */
.table {
	table-layout: fixed;
}

.pagination>li>a, .pagination>.disabled>a {
	color: black;
	background-color: #A9A9A9;
}
/*分页选择颜色  */
.pagination>li>a:hover, .pagination>li>span:hover, .pagination>li>a:focus,
	.pagination>li>span:focus {
	background-color: #333333
}
/* 分页框线 */
.fixed-table-pagination, .pagination>li>a, .pagination>li>span {
	border: 0px solid #615252;
}
/*页脚部分的背景色  */
.fixed-table-body, .fixed-table-loading, .fixed-table-pagination {
	background-color: #A9A9A9;
}
/* 表格动态选择颜色 */
.table-hover>tbody>tr:hover>td, .table-hover>tbody>tr:hover>th {
	background-color: #808080;
}

/*表格选中  */
.fixed-table-container tbody .selected td {
	background-color: #808080;
}
/* 框线 */
.fixed-table-container {
	border: 0.1px solid #615252;
	padding-bottom: 10%;
}

.pagination {
	padding-right: 1px;
}

.modal-dialog {
	right: auto;
	left: 50%;
	width: 500px;
	padding-top: 30px;
	padding-bottom: 30px;
}
/* 上传预览框 */
.pagination-detail .page-list {
	display: none;
}
/*过长的数据  */
.table tbody td {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.targetTabel tbody td {
	visible: visible;
	text-overflow: clip;
	white-space: nowrap;
}
/* 右侧最大框 */
.localRightpage {
	background-color: #A9A9A9;
	position: absolute;
	top: 10%;
	left: 15%;
	width: 84%;
	height: 88%;
/* 	min-height: 598px;
	min-width: 930px; */
}
/* 右侧中型框 */
.localRightview {
	position: absolute;
	background-color: #A9A9A9;
	top: 5%;
	width: 100%;
	height: 90%;
}

.selectp{
/* 	min-width: 915px;
 */}
/* 表格标题 */
.th-inner {
	color: black;
	text-align: right;
	padding: 6px;
}
/*表格内部字体样式 */
#cusTable>tbody>tr>td {
	padding: 5px;
	font-size: 14px;
	text-align: right;
	border: 0.5px solid #615252;
}
/* 标题样式 */
#cusTable>thead>tr>th {
	padding-right: 6px;
	font-size: 14px;
	border: 0.5px solid #615252;
}

/* target页表格↓ */
/* 目标界面特殊处理 */
.localRightpageTarget {
	background-color: #A9A9A9;
	position: absolute;
	top: 10%;
	left:15%;
	width: 84%;
	height: 88%;
/* 	min-height: 587px;
 */}

.localRightviewTarget {
