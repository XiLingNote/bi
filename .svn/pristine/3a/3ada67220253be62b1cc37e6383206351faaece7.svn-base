<!-- oa数据库 -->
	<select id="queryOaShopSaleData" resultType="map">
		SELECT SUM(payment) as saleGmv,COUNT(*) as orderNumber from oa.jd_syn_order
		where
		seller_nick=#{sellerNick}
		AND
		(
		pay_time!=0
		and
		pay_time
		BETWEEN
		UNIX_TIMESTAMP(#{startDate}) AND
		UNIX_TIMESTAMP(#{endDate})
		)
		or(
		pay_time=0
		AND
		created
		BETWEEN
		UNIX_TIMESTAMP(#{startDate}) AND
		UNIX_TIMESTAMP(#{endDate})
		)

	</select>
