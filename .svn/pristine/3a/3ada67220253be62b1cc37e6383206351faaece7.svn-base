	<!-- #本月发货 包含所有的 无全盘扫描 -->
	<select id="queryMonthShipmentAmount" resultType="java.lang.Integer">
		SELECT
		COUNT(DISTINCT orderInfo.order_id)
		FROM
		${repertoryName}.order_info orderInfo
		LEFT JOIN
		${repertoryName}.psendlk psendlk ON
		orderInfo.order_id =
		psendlk.order_id
		LEFT JOIN ${repertoryName}.check_pick_new
		checkPickNew
		ON orderInfo.order_id =
		checkPickNew.order_id and
		(
		psendlk.check_two = 1 OR
		orderInfo.shipping_status &gt;0
		)
		WHERE checkPickNew.scan_time
		BETWEEN
		UNIX_TIMESTAMP(DATE_FORMAT(NOW(),'%Y-%m-01'))
		AND
		UNIX_TIMESTAMP(NOW())
	</select>
	<!-- #上月发货数 无all -->
	<select id="queryLastMonthShipmentAmount" resultType="java.lang.Integer">
		SELECT
		COUNT(DISTINCT orderInfo.order_id)
		FROM
		${repertoryName}.order_info orderInfo
		LEFT JOIN
		${repertoryName}.psendlk psendlk ON
		orderInfo.order_id =
		psendlk.order_id
		LEFT JOIN ${repertoryName}.check_pick_new
		checkPickNew
		ON orderInfo.order_id =
		checkPickNew.order_id and
		(
		psendlk.check_two = 1 OR
		orderInfo.shipping_status &gt;0
		)
		WHERE
		PERIOD_DIFF(
		DATE_FORMAT(
		NOW( ) , '%Y%m' ) ,
		DATE_FORMAT(
		FROM_UNIXTIME(checkPickNew.scan_time), '%Y%m'
		))=1
	</select>
	<!-- #今日总单量 无all -->
	<select id="queryTotalSingleAmount" resultType="java.lang.Integer">
		SELECT count(*)
		from
		sys_info.jdp_tb_trade where (seller_nick=#{shopName} and
		jdp_modified>=DATE_FORMAT(NOW(),"%Y-%m-%d") and jdp_response like
		CONCAT('%"pay_time":"',DATE_FORMAT(NOW(),"%Y-%m-%d"),'%'))
		OR
		(seller_nick=#{shopName} and
		jdp_modified>=DATE_FORMAT(NOW(),"%Y-%m-%d") and jdp_response like
		CONCAT('%"pay_time":"',DATE_FORMAT(NOW(),"%Y-%m-%d"),'%')
		and
		STATUS!='WAIT_BUYER_PAY'and type='step')
	</select>
	<!-- #未转单数目 无all -->
	<select id="queryNotTurnAmount" resultType="java.lang.Integer">
		<!-- SELECT count(*) from( SELECT tid ,STATUS from sys_info.jdp_tb_trade 
			where seller_nick=#{shopName} and status="WAIT_SELLER_SEND_GOODS" ) jdp LEFT 
			JOIN ${repertoryName}.tb_push_status tps ON jdp.tid=tps.tid where tps.status!=1 -->
		SELECT count(DISTINCT jdp.tid) from sys_info.jdp_tb_trade jdp
		INNER
		JOIN ${repertoryName}.tb_push_status tps ON jdp.tid=tps.tid
		where
		jdp.seller_nick=#{shopName} and jdp.status="WAIT_SELLER_SEND_GOODS"
		and (tps.status!=1 or tps.`status` is NULL)
	</select>
	<!-- #今日转单数 payTime需要改为addTime -->
	<select id="queryTurnAmount" resultType="java.lang.Integer">
		SELECT count(*) from
		${repertoryName}.order_info ord
		left join
		${repertoryName}.tb_push_status st on
		st.tid=ord.deal_code where
		ord.pay_time
		BETWEEN
		UNIX_TIMESTAMP(DATE_FORMAT(NOW(),'%Y-%m-%d')) AND
		UNIX_TIMESTAMP(date_add(DATE_FORMAT(NOW(),'%Y-%m-%d'), interval 1
		day))
		AND st.status=1
	</select>
	<!-- #已经完成转单未打印 -->
	<select id="queryNotPrintAmount" resultType="java.lang.Integer">
		SELECT
		count(*)
		FROM ${repertoryName}.order_info ord
		LEFT JOIN
		${repertoryName}.tb_push_status AS st ON st.tid = ord.deal_code
		LEFT
		JOIN ${repertoryName}.psendlk AS p ON ord.order_id = p.order_id
		WHERE
		ord.order_status!=3
		AND p.is_print &lt; 1 
		AND ord.shipping_status &lt; 1
		
		<!-- SELECT count(*) FROM( SELECT ord.order_id as oid from ${repertoryName}.order_info 
			ord left join ${repertoryName}.tb_push_status as st on st.tid=ord.deal_code 
			where st.status=1 ) as ordtid LEFT JOIN ${repertoryName}.psendlk as p on 
			ordtid.oid=p.order_id where p.is_print &lt; 1 or p.is_print is null -->
	</select>
	<!-- #已经打印未发货配货中 -->
	<select id="queryDistributionAmount" resultType="java.lang.Integer">

		<!-- SELECT count(*) FROM( SELECT ord.order_id as oid from ${repertoryName}.order_info 
			ord left join ${repertoryName}.tb_push_status st on st.tid=ord.deal_code 
			where st.status=1 ) as ordtid LEFT JOIN ${repertoryName}.psendlk p on ordtid.oid=p.order_id 
			where p.is_print is not null and p.is_print &gt;0 and p.check_two!=1 -->
		SELECT
		count(DISTINCT ord.order_id)
		FROM
		${ repertoryName }.order_info
		ord
		LEFT JOIN ${ repertoryName
		}.tb_push_status st ON st.tid =
		ord.deal_code
		LEFT JOIN ${
		repertoryName }.psendlk p ON ord.order_id =
		p.order_id
		WHERE
		st. STATUS = 1
		AND p.is_print IS NOT NULL
		AND p.is_print
		&gt;0
		AND p.check_two != 1
		AND ord.order_status!=3
		AND ord.shipping_status &lt; 1
	</select>
	<!-- 今日发货数非预售 无all -->
	<select id="queryTodayShippedGeneralAmount" resultType="java.lang.Integer">
		SELECT
		COUNT(DISTINCT orderInfo.order_id)
		FROM
		${repertoryName}.order_info
		orderInfo
		LEFT JOIN ${repertoryName}.psendlk psendlk ON
		orderInfo.order_id = psendlk.order_id And
		orderInfo.presell = 0
		LEFT
		JOIN ${repertoryName}.check_pick_new checkPickNew ON
		orderInfo.order_id =
		checkPickNew.order_id and psendlk.check_two = 1
		WHERE
		checkPickNew.scan_time BETWEEN
		UNIX_TIMESTAMP(DATE_FORMAT(NOW(),'%Y-%m-%d')) AND
		UNIX_TIMESTAMP(date_add(DATE_FORMAT(NOW(),'%Y-%m-%d'), interval 1
		day))
	</select>
	<!-- #今日发货数预售 无all -->
	<select id="queryTodayShippedPresellAmount" resultType="java.lang.Integer">
		SELECT
		COUNT(DISTINCT orderInfo.order_id)
		FROM
		${repertoryName}.order_info
		orderInfo
		LEFT JOIN ${repertoryName}.psendlk psendlk ON
		orderInfo.order_id = psendlk.order_id And
		orderInfo.presell = 1
		LEFT
		JOIN ${repertoryName}.check_pick_new checkPickNew ON
		orderInfo.order_id =
