	<!-- 根据部门查询shop -->
	<select id="queryShopByscShort" resultType="ShopBean">
	SELECT
	shop.*
	FROM
	shop
	LEFT JOIN sale_cate salecate ON shop.sc_id = salecate.id
	WHERE
	shop.ex = 1
	AND shop.display_state=1
	AND salecate.ex = 1
	and salecate.short=#{scShort}
	</select>
